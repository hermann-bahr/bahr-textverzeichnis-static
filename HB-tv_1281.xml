                  </bibl>
                  <bibl>
                     <title level="j" ref="pmb62394">Das Kleine Journal</title>
                     <biblScope unit="jg">39</biblScope>
                     <date type="year">1917</date>
                     <biblScope unit="issue">64</biblScope> 4. <date when="1917-03-16">16.3.1917</date>
                  </bibl>
                  <bibl>
                     <title level="j" ref="pmb41289">Berliner Börsen-Courier</title>
                     <biblScope unit="jg">49</biblScope>
