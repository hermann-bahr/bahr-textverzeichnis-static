               <monogr>
                  <title level="j" ref="#41531">Berliner Tageblatt</title>
                  <imprint>
                     <biblScope unit="jg">44</biblScope>
                     <biblScope unit="issue">114, Abend-Ausgabe</biblScope>
                     <biblScope unit="page">2</biblScope>
                     <date when="1915-03-03">3.3.1915</date>
                  </imprint>
