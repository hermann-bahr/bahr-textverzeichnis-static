                  <title level="m">Sendung des Künstlers</title>
                  <imprint>
                     <pubPlace>Leipzig</pubPlace>
                     <publisher>Insel</publisher>
                     <date when="1923">1923</date>
                     <biblScope unit="pages">100–121</biblScope>
