               <monogr>
                  <title level="j" ref="#41471">Deutsche Zeitung</title>
                  <imprint>
                     <biblScope unit="jg">22</biblScope>
                     <biblScope unit="issue">7512, Morgen-Ausgabe</biblScope>
                     <biblScope unit="page">1–2</biblScope>
