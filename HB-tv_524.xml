                  <title level="j" ref="#41321">Neues Wiener Tagblatt</title>
                  <imprint>
                     <biblScope unit="jg">33</biblScope>
                     <biblScope unit="issue">338</biblScope>
                     <biblScope unit="pages">1–2</biblScope>
                     <date when="1899-12-08">8.12.1899</date>
                  </imprint>
