                     <ptr type="pmb" target="pmb26554"/>
                  </listRef>
               </ref>
               <ref type="URL">https://anno.onb.ac.at/cgi-content/anno?aid=nwg&amp;datum=19000914&amp;seite=8</ref>
               <noteGrp type="keywords">
