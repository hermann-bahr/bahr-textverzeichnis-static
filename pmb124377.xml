                  <title level="j" ref="pmb41244">Die Fackel</title>
                  <imprint>
                     <biblScope unit="jg">32</biblScope>
                     <biblScope unit="issue">838-844</biblScope>
                     <biblScope unit="page">8–11</biblScope>
                     <date when="1930">1930</date>
                  </imprint>
