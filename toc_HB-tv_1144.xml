         <monogr xmlns:tei="http://www.tei-c.org/ns/1.0">
            <title level="j">Süddeutsche Monatshefte</title>
            <imprint>
               <biblScope unit="jg">26</biblScope>
               <biblScope unit="issue">7 (April)</biblScope>
               <biblScope unit="pages">545–546</biblScope>
               <date when="1929-03-25">25.3.1929</date>
            </imprint>
