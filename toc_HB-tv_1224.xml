            <imprint>
               <biblScope unit="jg">69</biblScope>
               <biblScope unit="issue">959, 1. Morgenblatt</biblScope>
               <biblScope unit="pages">1–2</biblScope>
               <date when="1924-12-24">24.12.1924</date>
