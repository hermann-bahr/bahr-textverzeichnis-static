         <monogr xmlns:tei="http://www.tei-c.org/ns/1.0">
            <title level="j" ref="#36149">Freie Bühne für modernes Leben</title>
            <imprint>
               <biblScope unit="jg">1</biblScope>
               <biblScope unit="issue">22</biblScope>
               <biblScope unit="pages">596–598</biblScope>
               <date when="1890-07-02">2.7.1890</date>
