         <monogr xmlns:tei="http://www.tei-c.org/ns/1.0">
            <title level="m">Kritik der Gegenwart</title>
            <imprint>
               <pubPlace>Augsburg</pubPlace>
               <publisher>Haas &amp; Grabherr</publisher>
               <date when="1922">1922</date>
